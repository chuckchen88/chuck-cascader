{
  "name": "chuck-cascader",
  "version": "1.0.3",
  "description": "支持多选的移动端级联选择器",
  "author": "chuckchen",
  "private": false,
  "type": "module",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist"
  ],
  "main": "./dist/chuck-cascader.umd.cjs",
  "module": "./dist/chuck-cascader.js",
  "exports": {
    "./dist/style.css": "./dist/style.css",
    "./css": "./dist/style.css",
    ".": {
      "import": "./dist/chuck-cascader.js",
      "require": "./dist/chuck-cascader.umd.cjs"
    }
  },
  "keywords": [
    "cascader",
    "级联选择器",
    "移动端级联选择器",
    "支持多选"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chuckchen88/chuck-cascader"
  },
  "bugs": {
    "url": "https://github.com/chuckchen88/chuck-cascader/issues"
  },
  "homepage": "https://github.com/chuckchen88/chuck-cascader/blob/master/README.md",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:docs": "vue-tsc && vite build -- docs",
    "preview": "vite preview"
  },
  "license": "ISC",
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.4.2"
  }
}
